<ServerManagerConfiguration>
  <ProxyGroup name="views">
    <RenderViewProxy name="RGBZView"
        label="RGB+Z Exporter"
        class="vtkPVRenderViewForAssembly"
        processes="client|renderserver|dataserver"
        base_proxygroup="views" base_proxyname="RenderView">
        <StringVectorProperty
            name="ZOrdering"
            animateable="0"
            command="GetZOrdering"
            number_of_elements="1"
            information_only="1">
        </StringVectorProperty>
        <StringVectorProperty
        name="RepresentationCodes"
        animateable="0"
        command="GetRepresentationCodes"
        number_of_elements="1"
        information_only="1">
        </StringVectorProperty>

        <StringVectorProperty
            name="CompositeDirectory"
            animateable="0"
            command="SetCompositeDirectory"
            number_of_elements="1">
        </StringVectorProperty>
        <StringVectorProperty
            name="ImageFormatExtension"
            animateable="0"
            default_values="jpg"
            command="SetImageFormatExtension"
            number_of_elements="1">
        </StringVectorProperty>
        <IntVectorProperty
            name="RGBStackSize"
            animateable="0"
            command="SetRGBStackSize"
            default_values="-1"
            number_of_elements="1">
        </IntVectorProperty>
        <DoubleVectorProperty
            name="ClippingBounds"
            animateable="0"
            command="SetClippingBounds"
            default_values="+1 -1 +1 -1 +1 -1"
            number_of_elements="6">
        </DoubleVectorProperty>
        <Property
            name="ResetClippingBounds"
            animateable="0"
            command="ResetClippingBounds">
        </Property>
        <Property
            name="FreezeGeometryBounds"
            animateable="0"
            command="FreezeGeometryBounds">
        </Property>
        <Property
            name="ComputeZOrdering"
            animateable="0"
            command="ComputeZOrdering">
        </Property>
        <Property
            name="WriteImage"
            animateable="0"
            command="WriteImage">
        </Property>
        <Property
            name="CaptureActiveRepresentation"
            animateable="0"
            command="CaptureActiveRepresentation">
        </Property>
        <Property
            name="ResetActiveImageStack"
            animateable="0"
            command="ResetActiveImageStack">
        </Property>
        <Property
            name="WriteComposite"
            animateable="0"
            command="WriteComposite">
        </Property>
        <ProxyProperty command="AddRepresentationForComposite"
                       name="Representations"
                       remove_command="RemoveRepresentationForComposite"
                       override="1"
                       repeatable="1">
          <Documentation>Representations in this view.</Documentation>
        </ProxyProperty>
        <ProxyProperty command="SetActiveRepresentationForComposite"
                       name="ActiveRepresentation"/>

        <Property
            name="StartCaptureValues"
            animateable="0"
            command="StartCaptureValues">
        </Property>
        <IntVectorProperty
            name="SetDrawCells"
            animateable="0"
            command="SetDrawCells"
            default_values="0"
            number_of_elements="1">
        </IntVectorProperty>
        <StringVectorProperty
            name="SetArrayNameToDraw"
            animateable="0"
            command="SetArrayNameToDraw"
            default_values=""
            number_of_elements="1">
        </StringVectorProperty>
        <IntVectorProperty
            name="SetArrayNumberToDraw"
            animateable="0"
            command="SetArrayNumberToDraw"
            default_values="0"
            number_of_elements="1">
        </IntVectorProperty>
        <IntVectorProperty
            name="SetArrayComponentToDraw"
            animateable="0"
            command="SetArrayComponentToDraw"
            default_values="0"
            number_of_elements="1">
        </IntVectorProperty>
        <DoubleVectorProperty
            name="SetScalarRange"
            argument_is_array="0"
            animateable="0"
            command="SetScalarRange"
            default_values="0.0 -1.0"
            number_of_elements="2">
        </DoubleVectorProperty>

        <Property
            name="StopCaptureValues"
            animateable="0"
            command="StopCaptureValues">
        </Property>

    </RenderViewProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
